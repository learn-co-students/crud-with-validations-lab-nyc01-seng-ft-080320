
<% if flash[:errors] %>
    <% flash[:errors].each do |e| %>
        <%= e %>
    <% end %>
<% end %>
    

<%= form_for @song do |f| %>
  <%= f.label :title, "Title" %>:
  <%= f.text_field :title, list: 'title' %>
  <datalist id='title'>
    <% Song.all.each do |song| %>
      <option value="<%= song.title %>"></option>
    <% end %>
  </datalist>
  
  <%= f.label :released %>:
  <%= f.check_box :released %>
 

  <%= f.label :release_year, "Release Year" %>:
  <%= f.text_field :release_year, list: 'release_year' %>
  <datalist id='release_year'>
    <% Song.all.each do |song| %>
      <option value="<%= song.release_year %>"></option>
    <% end %>
  </datalist>

  <%= f.label :artist_name, "Artist" %>:
  <%= f.text_field :artist_name, list: 'artist_name' %>
  <datalist id='artist_name'>
    <% Song.all.each do |song| %>
      <option value="<%= song.artist_name %>"></option>
    <% end %>
  </datalist>
  
  <%= f.label :genre, "Genre" %>:
  <%= f.text_field :genre, list: 'genre' %>
  <datalist id='genre'>
    <% Song.all.each do |song| %>
      <option value="<%= song.genre %>"></option>
    <% end %>
  </datalist>
  
  <%= f.submit %>
<% end %>
