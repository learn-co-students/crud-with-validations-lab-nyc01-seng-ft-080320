<h1> All Songs </h1>

<table> 
    <thread>
    <tr>
        <th> Title</th>
        <th> Released</th>
        <th> Release Year</th>
        <th> Artist Name</th>
        <th> Genre</th>
    </tr>
    </thread>
    <tbody>
    <% @songs.each do |s| %>
    <tr>
       <td> <%= s.title %> </td>
       <td> <%= s.released %> </td>  
       <td> <%= s.release_year %> </td> 
       <td> <%= s.artist_name %> </td> 
       <td> <%= s.genre %> </td> 
    
    <td> <%= link_to 'View', song_path(s) %> |
          <%= link_to 'Edit', edit_song_path(s) %> |
          <%= link_to 'Delete', s, method: :delete, data: { confirm: 'For real?' } %> 
    </td>
    </tr>
    <% end %>
</tbody>
    
</table>


